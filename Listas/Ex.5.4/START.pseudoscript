# Transponha uma matriz
funcao transpor(lista):
    linhas = tamanho(lista)
    colunas = tamanho(lista[0])
    matrizresultado = []
    por cada i de 0 a linhas:
        for j de 0 a colunas:
            matrizresultado.adicionar(lista[i], lista[j])

    return matrizresultado

matrizA = [x[1,2], y[3,4],z[5,6]]

imprime(transpor(matrizA))